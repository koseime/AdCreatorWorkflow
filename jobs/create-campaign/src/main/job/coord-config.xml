<configuration>

    <!-- BEGIN Standard oozie configuration properties -->
    <property>
        <name>user.name</name>
        <value>@hadoopUser@</value>
    </property>
    <property>
        <name>oozie.coord.application.path</name>
        <value>@workflowRootUri@</value>
    </property>
    <property>
        <name>oozie.use.system.libpath</name>
        <value>true</value>
    </property>
    <property>
        <name>oozie.wf.rerun.failnodes</name>
        <value>true</value>
    </property>
    <property>
        <name>queueName</name>
        <value>default</value>
    </property>
    <!-- END Standard oozie configuration properties -->

    <!-- BEGIN CUSTOM job properties -->
    <property>
        <name>projectTempDataPath</name>
        <value>${environmentRootPath}/temp/${projectName}</value>
